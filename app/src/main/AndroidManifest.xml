<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.yinbao.www.listviewstyle"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ListView.text.TextListView"/>
        <activity android:name=".ListView.img.ImgListView"/>
        <activity android:name=".ListView.text.TextRecyclerView"/>
        <activity android:name=".ListView.img.ImgRecyclerView"/>
        <activity android:name=".ListView.group.TextGroupListView"/>
        <activity android:name=".ListView.group.TextGroupRecyclerView"/>
        <activity android:name=".ListView.select.SelectListView"/>
        <activity android:name=".ListView.twoList.TwoListView"/>
    </application>

</manifest>